	<relations>
		<!-- 供应商和商品一对多关系 -->
		<relation code="AcerChemVendor2Product" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="acerChemVendor" type="Vendor"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</sourceElement>
			<targetElement qualifier="acerChemProducts" type="Product"
				cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>

		<!-- 用户等级和用户一对多关系 -->
		<relation code="AcerChemCustomerLevel2User" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="userLevel" type="UserLevel"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</sourceElement>
			<targetElement qualifier="customers" type="User"
				cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>

		<!-- 员工和用户一对多关系 -->
		<relation code="Receptionist2Customer" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="relatedCustomer" type="Employee"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</sourceElement>
			<targetElement qualifier="receptionist" type="Customer"
				cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>

		<!-- 信用账户和流水记录一对多关系 -->
		<relation code="Transactions2CreditAccount" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="creditAccount" type="CustomerCreditAccount"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</sourceElement>
			<targetElement qualifier="transactions" type="CreditTransaction"
				cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>
        
        <!-- 客户和信用账户关系建立 -->
		<relation code="CreditAccount2Customer" generate="true" localized="false" autocreate="true">
			<sourceElement qualifier="creditAccount" type="CustomerCreditAccount" cardinality="one" >
				<modifiers read="true" write="true" search="true" optional="true" />
			</sourceElement>
			<targetElement qualifier="customer" type="Customer" cardinality="many" collectiontype="list" ordered="true">
				<modifiers read="true" write="true" search="true" optional="true" />
			</targetElement>
		</relation>

	</relations>

