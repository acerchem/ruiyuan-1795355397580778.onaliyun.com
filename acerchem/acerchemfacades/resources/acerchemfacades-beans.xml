<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.acerchem.facades.product.data.GenderData>"/>
		<property name="chemicalInfo" type="String"/>
		<property name="unitCalculateRato" type="String"/>
	</bean>

	<bean class="com.acerchem.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	
	<!-- shaun.sheng -->
	<!--创建futureStockLevelData添加两个字段  -->
	<bean class="com.acerchem.facades.product.data.FutureStockLevelData">
		<property name="futureStockLevel" type="java.lang.Integer"/>
		<property name="releaseDay" type="java.lang.Integer"/>
	</bean>
	
	
	<!--在stockData中添加字段(list)  -->
	<bean class="de.hybris.platform.commercefacades.product.data.StockData">
		<property name="futureStock" type="java.util.List&lt;com.acerchem.facades.product.data.FutureStockLevelData>"/>
	</bean>
	<!-- shaun.sheng -->

	<bean class="de.hybris.platform.commercefacades.order.data.OrderEntryData">
		<property name="warehouseCode" type="java.lang.String"/>
		<property name="isUseFutureStock" type="java.lang.Boolean"/>
	</bean>


	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData">
		<property name="addressDataList" type="java.util.List&lt;de.hybris.platform.commercefacades.user.data.AddressData>"/>
	</bean>


    <bean class="com.acerchem.facades.product.data.WarehouseData">
        <property name="code" type="String"/>
        <property name="name" type="String"/>
    </bean>

	<bean class="com.acerchem.facades.product.data.CountryToWarehouseData">
		<property name="storeId" type="String"/>
		<property name="countryData" type="de.hybris.platform.commercefacades.user.data.CountryData"/>
		<property name="warehouseList" type="java.util.List&lt;com.acerchem.facades.product.data.WarehouseData>"/>
	</bean>

    <bean class="com.acerchem.facades.product.data.CountryToWarehouseDataList">
        <property name="countryToWarehouseDataList" type="java.util.List&lt;com.acerchem.facades.product.data.CountryToWarehouseData>"/>
    </bean>

	<bean class="de.hybris.platform.commercefacades.order.data.AddToCartParams">
		<property name="warehouseCode" type="String"/>
		<property name="isUseFutureStock" type="Boolean"/>
	</bean>

	<bean class="de.hybris.platform.commerceservices.service.data.CommerceCartParameter">
		<property name="warehouseCode" type="String"/>
		<property name="isUseFutureStock" type="Boolean"/>
	</bean>

</beans>
