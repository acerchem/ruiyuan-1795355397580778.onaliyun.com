	<context merge-by="type" parent="GenericItem" type="ConsignmentEntry" component="create-wizard" merge-mode="replace">
		<wz:flow xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config" id="ConsignmentEntryConfigWizard">
			<wz:prepare id="ConsignmentEntryPrepare">
				<wz:initialize property="newConsignmentEntry" type="ctx.TYPE_CODE" /> 
				<wz:assign property="newConsignmentEntry.orderEntry" value="ctx.parentObject" />
			</wz:prepare>
			<wz:step id="step1" label="create.UniasiaConfig.general.label" merge-mode="replace">
				<wz:content id="step1.content" merge-mode="replace">
					<wz:property-list root="newConsignmentEntry" merge-mode="replace">
						<wz:property qualifier="orderEntry"  />
						<wz:property qualifier="quantity"  />
						<wz:property qualifier="batchNum"  />
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
						<wz:custom label="Done"  handler="acerchemSaveConsignmentHandler"/>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>
	
	<context component="create-wizard" type="UserLevel">
		<wz:flow xmlns:wz="http://www.hybris.com/cockpitng/config/wizard-config" id="UserLevelWizard" title="create.UserLevel.title">
			<wz:prepare id="UserLevelPrepare">
				<wz:initialize property="newUserLevel" type="UserLevel"/>
			</wz:prepare>
			<wz:step id="step1" label="create.UserLevel.general.label" >
				<wz:content id="step1.content">
					<wz:property-list root="newUserLevel">
						<wz:property qualifier="uid" />
