						<wz:property qualifier="levelName" />
						<wz:property qualifier="levelCode" />
						<wz:property qualifier="discount" />
					</wz:property-list>
				</wz:content>
				<wz:navigation id="step1.navigation">
					<wz:cancel/>
					<wz:done>
						<wz:save property="newUserLevel"/>
					</wz:done>
				</wz:navigation>
			</wz:step>
		</wz:flow>
	</context>
	
