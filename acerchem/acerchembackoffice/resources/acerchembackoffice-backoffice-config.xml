    <context component="create-wizard" type="StockLevel">
        <wz:flow id="StockLevelWizard" title="create.title(ctx.TYPE_CODE)">
            <wz:prepare id="stockLevelPrepare">
                <wz:initialize property="newStockLevel" type="ctx.TYPE_CODE"/>
                <wz:assign property="newStockLevel.product" value="ctx.parentObject" />
                <wz:assign property="newStockLevel.productCode" value="ctx.productCode"/>
                <wz:assign property="newStockLevel.warehouse" value="ctx.warehouse"/>
            </wz:prepare>
            <wz:step id="step1" label="create.stockLevel.essential.label" sublabel="create.stockLevel.essential.sublabel">
                <wz:content id="step1.content">
                    <wz:property-list root="newStockLevel">
                        <wz:property qualifier="productCode"/>
                        <wz:property qualifier="warehouse"/>
                        <wz:property qualifier="available"/>
                    </wz:property-list>
                </wz:content>
                <wz:navigation id="step1.navigation">
                    <wz:cancel/>
                    <wz:next visible="newStockLevel.productCode != null and newStockLevel.warehouse != null and
					    newStockLevel.available != null" default-target="step2"/>
                    <wz:done visible="newStockLevel.productCode != null and newStockLevel.warehouse != null and
					    newStockLevel.available != null">
                        <wz:save property="newStockLevel"/>
                    </wz:done>
                </wz:navigation>
            </wz:step>
            <wz:step id="step2" label="create.stockLevel.additional.label" sublabel="create.stockLevel.additional.sublabel">
                <wz:content id="step2.content">
                    <wz:property-list root="newStockLevel">
                        <wz:property qualifier="inStockStatus"/>
                        <wz:property qualifier="nextDeliveryTime"/>
                        <wz:property qualifier="releaseDate"/>
                        <wz:property qualifier="bin"/>
                    </wz:property-list>
                </wz:content>
                <wz:navigation id="step2.navigation">
                    <wz:cancel/>
                    <wz:back/>
                    <wz:done>
                        <wz:save property="newStockLevel"/>
                    </wz:done>
                </wz:navigation>
            </wz:step>
        </wz:flow>
    </context>
    

